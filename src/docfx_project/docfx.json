{
  "metadata": [
    {
      "src": [
        {
            "files": [
                "**/**/Dhgms.*.dll",
                "**/**/NetTestRegimentation.*.dll",
                "**/**/Vetuviem.*.dll",
                "**/**/Whipstaff.*.dll"
            ],
          "src": "../upstream/bin/"
        }
      ],
      "dest": "obj/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
    "build": {
        "content": [
            {
                "files": [ "**/*.yml" ],
                "src": "obj/api",
                "dest": "api"
            },
            {
                "files": [
                    "projects/**.md",
                    "projects/**.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_enableSearch": true,
            "pdf": true
        },
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern"
        ],
        "postProcessors": [],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "sitemap": {
            "baseUrl": "https://docs.dpvreony.com",
            "priority": 0.1,
            "changefreq": "monthly"
        }
    }
}
