{
  "metadata": [
    {
        "src": [
            {
                "files": [
                    "Whipstaff.sln"
                ],
                "src": "../../../whipstaff/src/"
            },
            {
                "files": [
                    "NetTestRegimentation.sln"
                ],
                "src": "../../../NetTestRegimentation/src/"
            },
            {
                "files": [
                    "Dhgms.Nucleotide.sln"
                ],
                "src": "../../../nucleotide/src/"
            },
            {
                "files": [
                    "Vetuviem.sln"
                ],
                "src": "../../../vetuviem/src/"
            }
        ],
      "dest": "obj/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
    "build": {
        "content": [
            {
                "files": [ "**/*.yml" ],
                "src": "obj/api",
                "dest": "api"
            },
            {
                "files": [
                    "**/*.yml",
                    "**/*.md"
                ],
                "src": "obj/gripe",
                "dest": "gripe"
            },
            {
                "files": [
                    "projects/**.md",
                    "projects/**.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_enableSearch": true
        },
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern"
        ],
        "postProcessors": [],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "sitemap": {
          "baseUrl": "https://docs.dpvreony.com",
          "priority": 0.1,
          "changefreq": "monthly"
        }
    }
}
