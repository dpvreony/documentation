{
  "metadata": [
    {
      "src": [
        {
            "files": [
                //"Whipstaff.AspNetCore.dll",
                "**/**/Dhgms.Nucleotide.*.dll",
                "**/**/NetTestRegimentation.*.dll",
                "**/**/Vetuviem.*.dll",
                "**/**/Whipstaff.*.dll"
            ],
          "src": "../upstream/bin/"
        }
      ],
      "dest": "obj/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
    "build": {
        "content": [
            {
                "files": [ "**/*.yml" ],
                "src": "obj/api",
                "dest": "api"
            },
            {
                "files": [
                    "articles/**.md",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_enableSearch": true
        },
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "markdownEngineProperties": {
            // "plantUml.localGraphvizDotPath": "C:/Program Files (x86)/Graphviz2.38/bin/dot.exe",
            "plantUml.localPlantUmlPath": "../../tools/plantuml.jar",
            //"plantUml.outputFormat": "png",
            "plantUml.remoteUrl": "http://www.plantuml.com/plantuml/",
            "plantUml.renderingMode": "remote"
        },
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
